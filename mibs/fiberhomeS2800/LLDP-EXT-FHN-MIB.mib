---


-- conformance information

lldpXFhnConformance OBJECT IDENTIFIER ::= { lldpXFhnMIB 2 }
lldpXFhnCompliances OBJECT IDENTIFIER ::= { lldpXFhnConformance 1 }
lldpXFhnGroups      OBJECT IDENTIFIER ::= { lldpXFhnConformance 2 }

-- compliance statements

lldpXFhnCompliance MODULE-COMPLIANCE
    STATUS  current
    DESCRIPTION
            "The compliance statement for SNMP entities which implement
            the LLDP Fhn extension MIB."
 
    MODULE  -- this module
        MANDATORY-GROUPS { lldpXFhnConfigGroup, 
                           lldpXFhnRemSysGroup
        }
        GROUP lldpXFhnOptSysOjbectIDGroup
            DESCRIPTION
                "This group represents the information associated with
                the LLDP-FHN optional Media Policy TLVs, 
                therefore the agent may not implement them."
           ::= { lldpXFhnCompliances 1 }

-- MIB groupings

lldpXFhnConfigGroup    OBJECT-GROUP
    OBJECTS {
        lldpXFhnPortCapSupported, 
        lldpXFhnPortConfigTLVsTxEnable
    }
    STATUS  current
    DESCRIPTION
            "The collection of objects which are used to configure or
            describe the configuration or behavior of the LLDP-FHN
            organizational extension implementation."
    ::= { lldpXFhnGroups 1 }

lldpXFhnOptSysOjbectIDGroup  OBJECT-GROUP
    OBJECTS {
        lldpXFhnLocSysObjectID 
        }
    STATUS  current
    DESCRIPTION
            "The collection of objects which are used to represent LLDP
            FHN organizational extension for Location Information."
    ::= { lldpXFhnGroups 2 }



lldpXFhnRemSysGroup  OBJECT-GROUP
    OBJECTS {
        lldpXFhnRemCapSupported,
        lldpXFhnRemCapCurrent,
        lldpXFhnRemSysObjectID
    }
     STATUS  current
    DESCRIPTION
            "The collection of objects which are used to represent LLDP-
            FHN organizational extension Remote Device Information."
    ::= { lldpXFhnGroups 3 }





END
