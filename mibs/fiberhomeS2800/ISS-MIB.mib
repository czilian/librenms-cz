        issIfRcvKeepAliveMultiplier OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The keepalive pkt receive timeout times. "
            ::= { issIfEntry 8 } 
            
	-- 1.3.6.1.4.1.3807.2.921.2.1.9
        issIfPeerState OBJECT-TYPE
            SYNTAX INTEGER
           		{
	           		down(0),
	           		up(1)
	           	}
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The peer state. "
            ::= { issIfEntry 9 }
            
	-- 1.3.6.1.4.1.3807.2.921.2.1.10
        issIfLocalState OBJECT-TYPE
            SYNTAX INTEGER
            	{
	           		down(0),
	           		up(1)
	           	}
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The local state. "
            ::= { issIfEntry 10 }
            
	-- 1.3.6.1.4.1.3807.2.921.2.1.11
        issIfProtocolState OBJECT-TYPE
            SYNTAX INTEGER
            	{
	           		down(0),
	           		up(1)
	           	}
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The protocal state. "
            ::= { issIfEntry 11 }
            
	-- 1.3.6.1.4.1.3807.2.921.2.1.12
        issIfMacAddress OBJECT-TYPE
            SYNTAX MacAddress
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The interface Mac address. "
            ::= { issIfEntry 12 }
          
          
-- ISS Topo Table   

	-- 1.3.6.1.4.1.3807.2.921.3                      
    	issTopoTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF ISSTopoEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	            "The iss topo table. "
	        ::= { iss 3 }
	        
	-- 1.3.6.1.4.1.3807.2.921.3.1                                                   
	     issTopoEntry OBJECT-TYPE
	         SYNTAX ISSTopoEntry
	         MAX-ACCESS not-accessible
	         STATUS current
	         DESCRIPTION
	            "The iss topo table. "
	         INDEX { issTopoIfIndex,issTopoHops }
	         ::= { issTopoTable 1 }
	         
		ISSTopoEntry ::=  
			SEQUENCE {
				issTopoIfIndex		INTEGER,
				issTopoHops			INTEGER,
				issTopoMemberID     INTEGER
			}
                                         
	-- 1.3.6.1.4.1.3807.2.921.3.1.1
        issTopoIfIndex OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Topo index 1. "
            ::= { issTopoEntry 1 }
            
	-- 1.3.6.1.4.1.3807.2.921.3.1.2
        issTopoHops OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Topo index 2. "
            ::= { issTopoEntry 2 }
            
	-- 1.3.6.1.4.1.3807.2.921.3.1.3
        issTopoMemberID OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The member id. "
            ::= { issTopoEntry 3 }
              
                                             
-- ISS Member Table   

	-- 1.3.6.1.4.1.3807.2.921.4                      
    	issMemberTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF ISSMemberEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	            "The iss member table. "
	        ::= { iss 4 }
	        
	-- 1.3.6.1.4.1.3807.2.921.4.1                                                   
	     issMemberEntry OBJECT-TYPE
	         SYNTAX ISSMemberEntry
	         MAX-ACCESS not-accessible
	         STATUS current
	         DESCRIPTION
	            "The iss member table. "
	         INDEX { issMemberIndex }
	         ::= { issMemberTable 1 }
                         
		ISSMemberEntry ::=
         	SEQUENCE { 
                issMemberIndex               		INTEGER,
                issMemberRole              			INTEGER,
                issMemberConfigRole             	INTEGER,
                issMemberPriority              		INTEGER,            
                issMemberMac    					MacAddress,
                issMemberRunningTime      			TimeTicks, 
                issMemberIfPort0State				INTEGER,
                issMemberIfPort1State    			INTEGER,
                issMemberIfPort0Mac        			MacAddress,
                issMemberIfPort1Mac          		MacAddress,
                issMemberIsLocal      				INTEGER,
                issMemberRunningState      			INTEGER
                  }    
                  
	-- 1.3.6.1.4.1.3807.2.921.4.1.1
        issMemberIndex OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "member index. "
            ::= { issMemberEntry 1 }  
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.2
        issMemberRole OBJECT-TYPE
            SYNTAX INTEGER
	           {
	           		slave(0),
	           		master(1)
	           }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "member role. "
            ::= { issMemberEntry 2 }
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.3
        issMemberConfigRole OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
