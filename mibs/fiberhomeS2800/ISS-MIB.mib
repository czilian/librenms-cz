	            "The system state. "
	        ::= { issCommon 18 }
       
	                
-- ISS Interface Table   

	-- 1.3.6.1.4.1.3807.2.921.2                      
    	issIfTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF ISSIfEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	            "The iss interface table. "
	        ::= { iss 2 }
	        
	-- 1.3.6.1.4.1.3807.2.921.2.1                                                   
	     issIfEntry OBJECT-TYPE
	         SYNTAX ISSIfEntry
	         MAX-ACCESS not-accessible
	         STATUS current
	         DESCRIPTION
	            "The iss interface table. "
	         INDEX { issIfIndex }
	         ::= { issIfTable 1 }
              
		ISSIfEntry ::=
         	SEQUENCE { 
                issIfIndex               		INTEGER,
                issIfStatus              		INTEGER,
                issIfBlockState             	INTEGER,
                issIfLinkState              	INTEGER,            
                issIfHops    					INTEGER,
                issIfKeepAliveInterval      	INTEGER,
                issIfPeerKeepAliveInterval      INTEGER, 
                issIfRcvKeepAliveMultiplier		INTEGER,
                issIfPeerState    				INTEGER,
                issIfLocalState        			INTEGER,
                issIfProtocolState          	INTEGER,
                issIfMacAddress      			MacAddress
                  }
              
	-- 1.3.6.1.4.1.3807.2.921.2.1.1
        issIfIndex OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Interface index. "
            ::= { issIfEntry 1 }  
            
	-- 1.3.6.1.4.1.3807.2.921.2.1.2
        issIfStatus OBJECT-TYPE
            SYNTAX INTEGER  
            {
            	enable(1),
            	disable(2)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The interface state. "
            ::= { issIfEntry 2 } 
            
	-- 1.3.6.1.4.1.3807.2.921.2.1.3
        issIfBlockState OBJECT-TYPE
            SYNTAX INTEGER
            	{
	           		down(0),
	           		up(1)
	           	}
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The block state. "
            ::= { issIfEntry 3 }
            
	-- 1.3.6.1.4.1.3807.2.921.2.1.4
        issIfLinkState OBJECT-TYPE
            SYNTAX INTEGER 
	           {
	           		up(1),
	           		down(2)
	           }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The link state. "
            ::= { issIfEntry 4 }
            
	-- 1.3.6.1.4.1.3807.2.921.2.1.5
        issIfHops OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The hops. "
            ::= { issIfEntry 5 }
            
