        issIfKeepAliveInterval OBJECT-TYPE
            SYNTAX INTEGER  (1..5)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The local keepalive pkt interval. "
            ::= { issIfEntry 6 }   
    
    -- 1.3.6.1.4.1.3807.2.921.2.1.7
        issIfPeerKeepAliveInterval OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The peer keepalive pkt interval. "
            ::= { issIfEntry 7 }
                    
