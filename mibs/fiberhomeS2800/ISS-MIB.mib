            ::= { issMemberEntry 3 }  
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.4
        issMemberPriority OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "member priority. "
            ::= { issMemberEntry 4 }       
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.5
        issMemberMac OBJECT-TYPE
            SYNTAX MacAddress
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "member mac. "
            ::= { issMemberEntry 5 }  
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.6
        issMemberRunningTime OBJECT-TYPE
            SYNTAX TimeTicks
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "member running time. "
            ::= { issMemberEntry 6 }
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.7
        issMemberIfPort0State OBJECT-TYPE
            SYNTAX INTEGER
            	{
	           		down(0),
	           		up(1)
	           	}
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "east interface state. "
            ::= { issMemberEntry 7 }  
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.8
        issMemberIfPort1State OBJECT-TYPE
            SYNTAX INTEGER
            	{
	           		down(0),
	           		up(1)
	           	}
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "west interface state. "
            ::= { issMemberEntry 8 }   
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.9
        issMemberIfPort0Mac OBJECT-TYPE
            SYNTAX MacAddress
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "east interface mac. "
            ::= { issMemberEntry 9 }
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.10
        issMemberIfPort1Mac OBJECT-TYPE
            SYNTAX MacAddress
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "west interface mac. "
            ::= { issMemberEntry 10 }  
            
	-- 1.3.6.1.4.1.3807.2.921.4.1.11
        issMemberIsLocal OBJECT-TYPE
            SYNTAX INTEGER
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "member is local or not. "
            ::= { issMemberEntry 11 }
            
   -- 1.3.6.1.4.1.3807.2.921.4.1.12
        issMemberRunningState OBJECT-TYPE
            SYNTAX INTEGER
            {
            	init(0),
	        	collection(1),
	        	election(2),
	        	loading(3),
	        	done(4)
            }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The member state. "
            ::= { issMemberEntry 12 }         
            
END