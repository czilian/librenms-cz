	        ::= { issCommon 3 }

	-- 1.3.6.1.4.1.3807.2.921.1.4
    	issIsRing OBJECT-TYPE
	        SYNTAX INTEGER  
	        MAX-ACCESS read-only
	        STATUS current
	        DESCRIPTION
