IPV6Priv-MIB DEFINITIONS ::= BEGIN

         IMPORTS
             MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
             mib-2, Counter32, Unsigned32, Integer32,
             Gauge32,enterprises                               FROM SNMPv2-SMI
             DisplayString, PhysAddress, TruthValue, TimeStamp,
             VariablePointer, RowPointer           FROM SNMPv2-TC
             MODULE-COMPLIANCE, OBJECT-GROUP,
             NOTIFICATION-GROUP                    FROM SNMPv2-CONF
             Ipv6IfIndex, Ipv6Address, Ipv6AddressPrefix,
             Ipv6AddressIfIdentifier,
             Ipv6IfIndexOrZero                     FROM IPV6-TC;
             
         wri MODULE-IDENTITY
							LAST-UPDATED 	"201107040000Z"
							ORGANIZATION 	"FiberHome Networks Systems, Inc."
							CONTACT-INFO	"http://www.fhn.com.cn"
							DESCRIPTION		"The Structure of Management Information for the FHN enterprise."
							::= { enterprises 3807 }	-- assigned by IANA
	
				wriProtocol           OBJECT IDENTIFIER ::= { wri 2 }
				wriIPv6               OBJECT IDENTIFIER ::= { wriProtocol 927 }

           
		-- the IPv6 general group
		

         ipv6PrivMIBObjects OBJECT IDENTIFIER   ::= { wriIPv6 1 }


         ipv6PrivForwarding OBJECT-TYPE
             SYNTAX      INTEGER {
                          forwarding(1),    -- acting as a router

                                            -- NOT acting as
                          notForwarding(2)  -- a router
                         }
              MAX-ACCESS read-write
              STATUS     current
              DESCRIPTION
                "The indication of whether this entity is acting
                as an IPv6 router in respect to the forwarding of
                datagrams received by, but not addressed to, this
                entity.  IPv6 routers forward datagrams.  IPv6
                hosts do not (except those source-routed via the
                host).
                Note that for some managed nodes, this object may
                take on only a subset of the values possible.
                Accordingly, it is appropriate for an agent to
                return a `wrongValue' response if a management
                station attempts to change this object to an
                inappropriate value."
                ::= { ipv6PrivMIBObjects 1 }

         ipv6PrivDefaultHopLimit OBJECT-TYPE
             SYNTAX      INTEGER(0..255)
             MAX-ACCESS  read-write
              STATUS     current
             DESCRIPTION
                "The default value inserted into the Hop Limit
                field of the IPv6 header of datagrams originated
                at this entity, whenever a Hop Limit value is not
                supplied by the transport layer protocol."
             DEFVAL  { 64 }
             ::= { ipv6PrivMIBObjects 2 }

        ipv6PrivInterfaces OBJECT-TYPE
             SYNTAX      Unsigned32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "The number of IPv6 interfaces (regardless of
                their current state) present on this system."
             ::= { ipv6PrivMIBObjects 3 }

        ipv6PrivIfTableLastChange OBJECT-TYPE
             SYNTAX      TimeStamp
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "The value of sysUpTime at the time of the last
               insertion or removal of an entry in the
               ipv6PrivIfTable. If the number of entries has been
               unchanged since the last re-initialization of
               the local network management subsystem, then this
               object contains a zero value."
             ::= { ipv6PrivMIBObjects 4 }


        -- the IPv6 Interfaces table

        ipv6PrivIfTable OBJECT-TYPE
             SYNTAX     SEQUENCE OF Ipv6PrivIfEntry
             MAX-ACCESS not-accessible
             STATUS     current
             DESCRIPTION
               "The IPv6 Interfaces table contains information
               on the entity's internetwork-layer interfaces.
               An IPv6 interface constitutes a logical network
               layer attachment to the layer immediately below
               IPv6 including internet layer 'tunnels', such as
               tunnels over IPv4 or IPv6 itself."
             ::= { ipv6PrivMIBObjects 5 }

         ipv6PrivIfEntry OBJECT-TYPE
             SYNTAX     Ipv6PrivIfEntry
             MAX-ACCESS not-accessible
             STATUS     current
             DESCRIPTION
               "An interface entry containing objects
                about a particular IPv6 interface."
             INDEX   { ipv6PrivIfIndex }
             ::= { ipv6PrivIfTable 1 }

         Ipv6PrivIfEntry ::= SEQUENCE {
                 ipv6PrivIfIndex              Ipv6IfIndex,
                 ipv6PrivIfDescr              DisplayString,
                 ipv6PrivIfLowerLayer         VariablePointer,
                 ipv6PrivIfEffectiveMtu       Unsigned32,
                 ipv6PrivIfReasmMaxSize       Unsigned32,
                 ipv6PrivIfIdentifier         Ipv6AddressIfIdentifier,
                 ipv6PrivIfIdentifierLength   INTEGER,
                 ipv6PrivIfPhysicalAddress    PhysAddress,
                 ipv6PrivIfAdminStatus        INTEGER,
                 ipv6PrivIfOperStatus         INTEGER,
                 ipv6PrivIfLastChange         TimeStamp,
				 ipv6PrivIfStatus         	  INTEGER,
				 ipv6PrivIfAutoLinkLocal      INTEGER
             }

         ipv6PrivIfIndex OBJECT-TYPE
             SYNTAX     Ipv6IfIndex
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
               "A unique non-zero value identifying
                the particular IPv6 interface."
             ::= { ipv6PrivIfEntry 1 }

         ipv6PrivIfDescr OBJECT-TYPE
             SYNTAX     DisplayString
             MAX-ACCESS read-write
             STATUS     current
             DESCRIPTION
               "A textual string containing information about the
               interface.  This string may be set by the network
               management system."
             ::= { ipv6PrivIfEntry 2 }

         ipv6PrivIfLowerLayer OBJECT-TYPE
         SYNTAX      VariablePointer
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
              "This object identifies the protocol layer over
               which this network interface operates.  If this
               network interface operates over the data-link
               layer, then the value of this object refers to an
               instance of ifIndex [6]. If this network interface
               operates over an IPv4 interface, the value of this
               object refers to an instance of ipAdEntAddr [3].

               If this network interface operates over another
               IPv6 interface, the value of this object refers to
               an instance of ipv6PrivIfIndex.  If this network
               interface is not currently operating over an active
               protocol layer, then the value of this object
               should be set to the OBJECT ID { 0 0 }."
            ::= { ipv6PrivIfEntry 3 }

         ipv6PrivIfEffectiveMtu OBJECT-TYPE
            SYNTAX      Unsigned32
            UNITS       "octets"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
              "The size of the largest IPv6 packet which can be
              sent/received on the interface, specified in
              octets."
         ::= { ipv6PrivIfEntry 4 }

         ipv6PrivIfReasmMaxSize OBJECT-TYPE
            SYNTAX      Unsigned32 (0..65535)
            UNITS       "octets"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
              "The size of the largest IPv6 datagram which this
              entity can re-assemble from incoming IPv6 fragmented
              datagrams received on this interface."
         ::= { ipv6PrivIfEntry 5 }

         ipv6PrivIfIdentifier OBJECT-TYPE
             SYNTAX      Ipv6AddressIfIdentifier
             MAX-ACCESS  read-write
             STATUS      current
             DESCRIPTION
                "The Interface Identifier for this interface that
                is (at least) unique on the link this interface is
                attached to. The Interface Identifier is combined
                with an address prefix to form an interface address.

                By default, the Interface Identifier is autoconfigured
                according to the rules of the link type this
                interface is attached to."
             ::= { ipv6PrivIfEntry 6 } 
             
         ipv6PrivIfIdentifierLength OBJECT-TYPE
             SYNTAX      INTEGER (0..64)
             UNITS       "bits"
             MAX-ACCESS  read-write
             STATUS      current
             DESCRIPTION
               "The length of the Interface Identifier in bits."
             ::= { ipv6PrivIfEntry 7 }

         ipv6PrivIfPhysicalAddress OBJECT-TYPE
             SYNTAX      PhysAddress
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "The interface's physical address. For example, for
               an IPv6 interface attached to an 802.x link, this
               object normally contains a MAC address. Note that
               in some cases this address may differ from the
               address of the interface's protocol sub-layer.  The
               interface's media-specific MIB must define the bit
               and byte ordering and the format of the value of
               this object. For interfaces which do not have such
               an address (e.g., a serial line), this object should
               contain an octet string of zero length."
             ::= { ipv6PrivIfEntry 8 }

        ipv6PrivIfAdminStatus OBJECT-TYPE
            SYNTAX  INTEGER {
                     up(1),       -- ready to pass packets
                     down(2)
                    }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
              "The desired state of the interface.  When a managed
              system initializes,  all IPv6 interfaces start with
              ipv6PrivIfAdminStatus in the down(2) state.  As a result
              of either explicit management action or per
              configuration information retained by the managed
              system,  ipv6PrivIfAdminStatus is then changed to
              the up(1) state (or remains in the down(2) state)."
            ::= { ipv6PrivIfEntry 9 }

        ipv6PrivIfOperStatus OBJECT-TYPE
            SYNTAX  INTEGER {
                     up(1),             -- ready to pass packets

                     down(2),

                     noIfIdentifier(3), -- no interface identifier

                                        -- status can not be
                                        -- determined for some
                     unknown(4),        -- reason

                                        -- some component is
                     notPresent(5)      -- missing
                    }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
              "The current operational state of the interface.
              The noIfIdentifier(3) state indicates that no valid
              Interface Identifier is assigned to the interface.
              This state usually indicates that the link-local
              interface address failed Duplicate Address Detection.
              If ipv6PrivIfAdminStatus is down(2) then ipv6PrivIfOperStatus
              should be down(2).  If ipv6PrivIfAdminStatus is changed
              to up(1) then ipv6PrivIfOperStatus should change to up(1)
              if the interface is ready to transmit and receive
              network traffic; it should remain in the down(2) or
              noIfIdentifier(3) state if and only if there is a
              fault that prevents it from going to the up(1) state;
              it should remain in the notPresent(5) state if
              the interface has missing (typically, lower layer)
              components."
            ::= { ipv6PrivIfEntry 10 }

        ipv6PrivIfLastChange OBJECT-TYPE
            SYNTAX      TimeStamp
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The value of sysUpTime at the time the interface
                entered its current operational state.  If the
                current state was entered prior to the last
                re-initialization of the local network management
                subsystem, then this object contains a zero
                value."
            ::= { ipv6PrivIfEntry 11 }

		ipv6PrivIfStatus OBJECT-TYPE
            SYNTAX      INTEGER
			{
				 enable(1),             
                 disable(2)
			}
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The ipv6 status of interface."
            ::= { ipv6PrivIfEntry 12 }
			
		ipv6PrivIfAutoLinkLocal OBJECT-TYPE
            SYNTAX      INTEGER
			{
				 true(1),             
                 false(2)
			}
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "The ipv6 status of link local."
            ::= { ipv6PrivIfEntry 13 }
			
         --  IPv6 Interface Statistics table

         ipv6PrivIfStatsTable OBJECT-TYPE
             SYNTAX     SEQUENCE OF Ipv6IfStatsEntry
             MAX-ACCESS not-accessible
             STATUS     current
             DESCRIPTION
                 "IPv6 interface traffic statistics."
             ::= { ipv6PrivMIBObjects 6 }

         ipv6PrivIfStatsEntry OBJECT-TYPE
             SYNTAX     Ipv6IfStatsEntry
             MAX-ACCESS not-accessible
             STATUS     current
             DESCRIPTION
                 "An interface statistics entry containing objects
                 at a particular IPv6 interface."
             AUGMENTS { ipv6PrivIfEntry }
             ::= { ipv6PrivIfStatsTable 1 }

         Ipv6IfStatsEntry ::= SEQUENCE {
                 ipv6PrivIfStatsInReceives
                     Counter32,
                 ipv6PrivIfStatsInHdrErrors
                     Counter32,
                 ipv6PrivIfStatsInTooBigErrors
                     Counter32,
                 ipv6PrivIfStatsInNoRoutes
                     Counter32,
                 ipv6PrivIfStatsInAddrErrors
                     Counter32,
                 ipv6PrivIfStatsInUnknownProtos
                     Counter32,
                 ipv6PrivIfStatsInTruncatedPkts
                     Counter32,
                 ipv6PrivIfStatsInDiscards
                     Counter32,
                 ipv6PrivIfStatsInDelivers
                     Counter32,
                 ipv6PrivIfStatsOutForwDatagrams
                     Counter32,
                 ipv6PrivIfStatsOutRequests
                     Counter32,
                 ipv6PrivIfStatsOutDiscards
                 Counter32,
                 ipv6PrivIfStatsOutFragOKs
                     Counter32,
                 ipv6PrivIfStatsOutFragFails
                     Counter32,
                 ipv6PrivIfStatsOutFragCreates
                     Counter32,
                 ipv6PrivIfStatsReasmReqds
                     Counter32,
                 ipv6PrivIfStatsReasmOKs
                     Counter32,
                 ipv6PrivIfStatsReasmFails
                     Counter32,
                 ipv6PrivIfStatsInMcastPkts
                     Counter32,
                 ipv6PrivIfStatsOutMcastPkts
                     Counter32,
           --private item
                 ipv6PrivIfStatsReset
                     INTEGER,                     
                 ipv6PrivIfStatsTcpInitiatedConns
                     Counter32,
                 ipv6PrivIfStatsTcpAcceptedConns
                     Counter32,
                 ipv6PrivIfStatsTcpEstablishedConns
                     Counter32,
                 ipv6PrivIfStatsTcpDroppedConns
                     Counter32,
                 ipv6PrivIfStatsTcpInitiatedDropConns
                     Counter32,
                 ipv6PrivIfStatsTcpClosedConns
                     Counter32,
                 ipv6PrivIfStatsTcpTimeoutDroppedConns
                     Counter32,
                 ipv6PrivIfStatsTcpRetransTimeout
                     Counter32,
                 ipv6PrivIfStatsTcpKeepAliveTimeout
                     Counter32,
                 ipv6PrivIfStatsTcpKeepAliveProbe
                     Counter32,
                 ipv6PrivIfStatsTcpDroppedInKeepalive
                     Counter32,
                 ipv6PrivIfStatsTcpInPackets
                 Counter32,
                 ipv6PrivIfStatsTcpInSequence
                     Counter32,
                 ipv6PrivIfStatsTcpInBytes
                     Counter32,
                 ipv6PrivIfStatsTcpInChecksumErrs
                     Counter32,
                 ipv6PrivIfStatsTcpInOffsetErrs
                     Counter32,
                 ipv6PrivIfStatsTcpInShortErrs
                     Counter32,
                 ipv6PrivIfStatsTcpInDuplicatePackets
                     Counter32,
                 ipv6PrivIfStatsTcpInDuplicateBytes
                     Counter32,
                 ipv6PrivIfStatsTcpInPartDuplicatePackets
                     Counter32,                     
                 ipv6PrivIfStatsTcpInPartDuplicateBytes
                     Counter32,
                 ipv6PrivIfStatsTcpInOutOrderPackets
                     Counter32,
                 ipv6PrivIfStatsTcpInOutOrderBytes
                     Counter32,
                 ipv6PrivIfStatsTcpInAfterWindowPackets
                     Counter32,
                 ipv6PrivIfStatsTcpInAfterWindowBytes
                     Counter32,
                 ipv6PrivIfStatsTcpInAfterClosePackets
                     Counter32,
                 ipv6PrivIfStatsTcpInWindowProbePacks
                     Counter32,
                 ipv6PrivIfStatsTcpInDuplicateAcks
                     Counter32,
                 ipv6PrivIfStatsTcpInTooMuchAcks
                     Counter32,
                 ipv6PrivIfStatsTcpInAckPackets
                     Counter32,
                 ipv6PrivIfStatsTcpInAckBytes
                     Counter32,
                 ipv6PrivIfStatsTcpInWindowUpdate
                 Counter32,
                 ipv6PrivIfStatsTcpOutDelayedAcks
                     Counter32,
                 ipv6PrivIfStatsTcpOutPackets
                     Counter32,
                 ipv6PrivIfStatsTcpOutDataPackets
                     Counter32,
                 ipv6PrivIfStatsTcpOutDataBytes
                     Counter32,
                 ipv6PrivIfStatsTcpOutRetransmitPackets
                     Counter32,
                 ipv6PrivIfStatsTcpOutRetransmitBytes
                     Counter32,
                 ipv6PrivIfStatsTcpOutAcks
                     Counter32,
                 ipv6PrivIfStatsTcpOutWindowProbes
                     Counter32,                     
                 ipv6PrivIfStatsTcpOutUrgentPackets
                     Counter32,
                 ipv6PrivIfStatsTcpOutWindowUpdates
                     Counter32,
                 ipv6PrivIfStatsTcpOutControls
                     Counter32,
                 ipv6PrivIfStatsTcpReset
                     INTEGER,
                 ipv6PrivIfStatsUdpInPackets
                     Counter32,
                 ipv6PrivIfStatsUdpInShorerThanHeaders
                     Counter32,
                 ipv6PrivIfStatsUdpInCheckError
                     Counter32,
                 ipv6PrivIfStatsUdpInInvalidLength
                     Counter32,
                 ipv6PrivIfStatsUdpInNoSocketOnPort
                     Counter32,                 
                 ipv6PrivIfStatsUdpInNoMulticastPort
                     Counter32,
                 ipv6PrivIfStatsUdpInSocketFull
                     Counter32,
                 ipv6PrivIfStatsUdpInPcbCacheMiss
                     Counter32,
                 ipv6PrivIfStatsUdpOutPackets
                     Counter32,
                 ipv6PrivIfStatsUdpReset
                     INTEGER
             }

         ipv6PrivIfStatsInReceives OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The total number of input datagrams received by
                the interface, including those received in error."
             ::= { ipv6PrivIfStatsEntry 1 }

         ipv6PrivIfStatsInHdrErrors OBJECT-TYPE
             SYNTAX     Counter32
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
                "The number of input datagrams discarded due to
                errors in their IPv6 headers, including version
                number mismatch, other format errors, hop count
                exceeded, errors discovered in processing their
                IPv6 options, etc."
             ::= { ipv6PrivIfStatsEntry 2 }

         ipv6PrivIfStatsInTooBigErrors OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "The number of input datagrams that could not be
               forwarded because their size exceeded the link MTU
               of outgoing interface."
               ::= { ipv6PrivIfStatsEntry 3 }

         ipv6PrivIfStatsInNoRoutes OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of input datagrams discarded because no
                 route could be found to transmit them to their
                 destination."
             ::= { ipv6PrivIfStatsEntry 4 }

         ipv6PrivIfStatsInAddrErrors OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of input datagrams discarded because
                the IPv6 address in their IPv6 header's destination
                field was not a valid address to be received at
                this entity.  This count includes invalid
                addresses (e.g., ::0) and unsupported addresses
                (e.g., addresses with unallocated prefixes).  For
                entities which are not IPv6 routers and therefore
                do not forward datagrams, this counter includes
                datagrams discarded because the destination address
                was not a local address."
             ::= { ipv6PrivIfStatsEntry 5 }

         ipv6PrivIfStatsInUnknownProtos OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of locally-addressed datagrams
                received successfully but discarded because of an
                unknown or unsupported protocol. This counter is
                incremented at the interface to which these
                datagrams were addressed which might not be
                necessarily the input interface for some of
                the datagrams."
             ::= { ipv6PrivIfStatsEntry 6 }


         ipv6PrivIfStatsInTruncatedPkts OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of input datagrams discarded because
                 datagram frame didn't carry enough data."
             ::= { ipv6PrivIfStatsEntry 7 }

         ipv6PrivIfStatsInDiscards OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of input IPv6 datagrams for which no
                problems were encountered to prevent their
                continued processing, but which were discarded
                (e.g., for lack of buffer space).  Note that this
                counter does not include any datagrams discarded
                while awaiting re-assembly."
             ::= { ipv6PrivIfStatsEntry 8 }

         ipv6PrivIfStatsInDelivers OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
              "The total number of datagrams successfully
              delivered to IPv6 user-protocols (including ICMP).
              This counter is incremented at the interface to
              which these datagrams were addressed which might
              not be necessarily the input interface for some of
              the datagrams."
             ::= { ipv6PrivIfStatsEntry 9 }

         ipv6PrivIfStatsOutForwDatagrams OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of output datagrams which this
                entity received and forwarded to their final
                destinations.  In entities which do not act
                as IPv6 routers, this counter will include
                only those packets which were Source-Routed
                via this entity, and the Source-Route
                processing was successful.  Note that for
                a successfully forwarded datagram the counter
                of the outgoing interface is incremented."
             ::= { ipv6PrivIfStatsEntry 10 }

         ipv6PrivIfStatsOutRequests OBJECT-TYPE
         	 SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
              "The total number of IPv6 datagrams which local IPv6
              user-protocols (including ICMP) supplied to IPv6 in
              requests for transmission.  Note that this counter
              does not include any datagrams counted in
              ipv6PrivIfStatsOutForwDatagrams."
             ::= { ipv6PrivIfStatsEntry 11 }

         ipv6PrivIfStatsOutDiscards OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                 "The number of output IPv6 datagrams for which no
                 problem was encountered to prevent their
                 transmission to their destination, but which were
                 discarded (e.g., for lack of buffer space).  Note
                 that this counter would include datagrams counted
                 in ipv6PrivIfStatsOutForwDatagrams if any such packets
                 met this (discretionary) discard criterion."
             ::= { ipv6PrivIfStatsEntry 12 }

         ipv6PrivIfStatsOutFragOKs OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of IPv6 datagrams that have been
                 successfully fragmented at this output interface."
             ::= { ipv6PrivIfStatsEntry 13 }

         ipv6PrivIfStatsOutFragFails OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of IPv6 datagrams that have been
                 discarded because they needed to be fragmented
                 at this output interface but could not be."
             ::= { ipv6PrivIfStatsEntry 14 }

         ipv6PrivIfStatsOutFragCreates OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of output datagram fragments that have
                 been generated as a result of fragmentation at
                 this output interface."
             ::= { ipv6PrivIfStatsEntry 15 }

         ipv6PrivIfStatsReasmReqds OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of IPv6 fragments received which needed
                 to be reassembled at this interface.  Note that this
                 counter is incremented at the interface to which
                 these fragments were addressed which might not
                 be necessarily the input interface for some of
                 the fragments."
             ::= { ipv6PrivIfStatsEntry 16 }

         ipv6PrivIfStatsReasmOKs OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "The number of IPv6 datagrams successfully
               reassembled.  Note that this counter is incremented
               at the interface to which these datagrams were
               addressed which might not be necessarily the input
               interface for some of the fragments."
             ::= { ipv6PrivIfStatsEntry 17 }

         ipv6PrivIfStatsReasmFails OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of failures detected by the IPv6 re-
                assembly algorithm (for whatever reason: timed
                out, errors, etc.).  Note that this is not
                necessarily a count of discarded IPv6 fragments
                since some algorithms (notably the algorithm in
                RFC 815) can lose track of the number of fragments
                by combining them as they are received.
                This counter is incremented at the interface to which
                these fragments were addressed which might not be
                necessarily the input interface for some of the
                fragments."
             ::= { ipv6PrivIfStatsEntry 18 } 
        ipv6PrivIfStatsInMcastPkts OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS     current
             DESCRIPTION
                "The number of multicast packets received
                 by the interface"
             ::= { ipv6PrivIfStatsEntry 19 }

         ipv6PrivIfStatsOutMcastPkts OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of multicast packets transmitted
                 by the interface"
             ::= { ipv6PrivIfStatsEntry 20 }

 --private item
	 ipv6PrivIfStatsReset OBJECT-TYPE
             SYNTAX      INTEGER (0..2)             
             MAX-ACCESS  read-write
             STATUS      current
             DESCRIPTION
               "Reset the ipv6Priv interface statistics."
             ::= { ipv6PrivIfStatsEntry 21 }       
        

         ipv6PrivIfStatsTcpInitiatedConns OBJECT-TYPE
             SYNTAX     Counter32
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
                ""
             ::= { ipv6PrivIfStatsEntry 22 }

         ipv6PrivIfStatsTcpAcceptedConns OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               ""
               ::= { ipv6PrivIfStatsEntry 23 }

         ipv6PrivIfStatsTcpEstablishedConns OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                ""
             ::= { ipv6PrivIfStatsEntry 24 }

         ipv6PrivIfStatsTcpDroppedConns OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                ""
             ::= { ipv6PrivIfStatsEntry 25 }

         ipv6PrivIfStatsTcpInitiatedDropConns OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of locally-addressed datagrams
                received successfully but discarded because of an
                unknown or unsupported protocol. This counter is
                incremented at the interface to which these
                datagrams were addressed which might not be
                necessarily the input interface for some of
                the datagrams."
             ::= { ipv6PrivIfStatsEntry 26 }


         ipv6PrivIfStatsTcpClosedConns OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "The number of input datagrams discarded because
                 datagram frame didn't carry enough data."
             ::= { ipv6PrivIfStatsEntry 27 }

         ipv6PrivIfStatsTcpTimeoutDroppedConns OBJECT-TYPE
