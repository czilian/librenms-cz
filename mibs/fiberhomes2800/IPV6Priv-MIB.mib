         -- Address Prefix table

         -- The IPv6 Address Prefix table contains information on
         -- the entity's IPv6 Address Prefixes that are associated
         -- with IPv6 interfaces.

         ipv6PrivAddrPrefixTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF Ipv6PrivAddrPrefixEntry
             MAX-ACCESS  not-accessible
             STATUS      current
             DESCRIPTION
                 "The list of IPv6 address prefixes of
                 IPv6 interfaces."
             ::= { ipv6PrivMIBObjects 7 }

         ipv6PrivAddrPrefixEntry OBJECT-TYPE
             SYNTAX  Ipv6PrivAddrPrefixEntry
             MAX-ACCESS  not-accessible
             STATUS      current
             DESCRIPTION
                 "An interface entry containing objects of
                 a particular IPv6 address prefix."
             INDEX   { ipv6PrivIfIndex,
                       ipv6PrivAddrPrefix,
                       ipv6PrivAddrPrefixLength }
             ::= { ipv6PrivAddrPrefixTable 1 }

         Ipv6PrivAddrPrefixEntry ::= SEQUENCE {
         	  ipv6PrivAddrPrefix                     Ipv6AddressPrefix,
              ipv6PrivAddrPrefixLength               INTEGER,
              ipv6PrivAddrPrefixOnLinkFlag           TruthValue,
              ipv6PrivAddrPrefixAutonomousFlag       TruthValue,
              ipv6PrivAddrPrefixAdvPreferredLifetime Unsigned32,
              ipv6PrivAddrPrefixAdvValidLifetime     Unsigned32
             }

         ipv6PrivAddrPrefix OBJECT-TYPE
             SYNTAX      Ipv6AddressPrefix
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "The prefix associated with the this interface."
             ::= { ipv6PrivAddrPrefixEntry 1 }

         ipv6PrivAddrPrefixLength OBJECT-TYPE
             SYNTAX      INTEGER (0..128)
             UNITS       "bits"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "The length of the prefix (in bits)."
             ::= { ipv6PrivAddrPrefixEntry 2 }

         ipv6PrivAddrPrefixOnLinkFlag OBJECT-TYPE
             SYNTAX      TruthValue
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "This object has the value 'true(1)', if this
               prefix can be used  for on-link determination
               and the value 'false(2)' otherwise."
             ::= { ipv6PrivAddrPrefixEntry 3 }

         ipv6PrivAddrPrefixAutonomousFlag OBJECT-TYPE
             SYNTAX      TruthValue
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "Autonomous address configuration flag. When
               true(1), indicates that this prefix can be used
               for autonomous address configuration (i.e. can
               be used to form a local interface address).
               If false(2), it is not used to autoconfigure
               a local interface address."
             ::= { ipv6PrivAddrPrefixEntry 4 }
             
           ipv6PrivAddrPrefixAdvPreferredLifetime OBJECT-TYPE
             SYNTAX      Unsigned32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
                "It is the length of time in seconds that this
                prefix will remain preferred, i.e. time until
                deprecation.  A value of 4,294,967,295 represents
                infinity.

                The address generated from a deprecated prefix
                should no longer be used as a source address in
                new communications, but packets received on such
                an interface are processed as expected."
             ::= { ipv6PrivAddrPrefixEntry 5 }

         ipv6PrivAddrPrefixAdvValidLifetime OBJECT-TYPE
             SYNTAX      Unsigned32
             UNITS       "seconds"
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "It is the length of time in seconds that this
               prefix will remain valid, i.e. time until
               invalidation.  A value of 4,294,967,295 represents
               infinity.

               The address generated from an invalidated prefix
               should not appear as the destination or source
               address of a packet."
             ::= { ipv6PrivAddrPrefixEntry 6 }


         -- the IPv6 Address table

         -- The IPv6 address table contains this node's IPv6
         -- addressing information.

         ipv6PrivAddrTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF Ipv6AddrEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
              "The table of addressing information relevant to
              this node's interface addresses."
            ::= { ipv6PrivMIBObjects 8 }
         ipv6PrivAddrEntry OBJECT-TYPE
            SYNTAX      Ipv6AddrEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "The addressing information for one of this
                node's interface addresses."
            INDEX   { ipv6PrivIfIndex, ipv6PrivAddrAddress }
            ::= { ipv6PrivAddrTable 1 }

         Ipv6AddrEntry ::=
            SEQUENCE {
                 ipv6PrivAddrAddress        Ipv6Address,
                 ipv6PrivAddrPfxLength      INTEGER,
                 ipv6PrivAddrType           INTEGER,
                 ipv6PrivAddrAnycastFlag    TruthValue,
                 ipv6PrivAddrStatus         INTEGER
                }

         ipv6PrivAddrAddress OBJECT-TYPE
            SYNTAX      Ipv6Address
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
              "The IPv6 address to which this entry's addressing
              information pertains."
            ::= { ipv6PrivAddrEntry 1 }

         ipv6PrivAddrPfxLength OBJECT-TYPE
            SYNTAX      INTEGER(0..128)
            UNITS       "bits"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
              "The length of the prefix (in bits) associated with
              the IPv6 address of this entry."
            ::= { ipv6PrivAddrEntry 2 }

         ipv6PrivAddrType OBJECT-TYPE
            SYNTAX      INTEGER {
                                -- address has been formed
                                -- using stateless
                 stateless(1),  -- autoconfiguration

                                -- address has been acquired
                                -- by stateful means
                                -- (e.g. DHCPv6, manual
                 stateful(2),   -- configuration)
                 
                                -- type can not be determined
                 unknown(3)     -- for some reason.
               }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
               "The type of address. Note that 'stateless(1)'
               refers to an address that was statelessly
               autoconfigured; 'stateful(2)' refers to a address
               which was acquired by via a stateful protocol
               (e.g. DHCPv6, manual configuration)."
            ::= { ipv6PrivAddrEntry 3 }

         ipv6PrivAddrAnycastFlag OBJECT-TYPE
             SYNTAX      TruthValue
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "This object has the value 'true(1)', if this
               address is an anycast address and the value
               'false(2)' otherwise."
             ::= { ipv6PrivAddrEntry 4 }

         ipv6PrivAddrStatus OBJECT-TYPE
            SYNTAX      INTEGER {
                     preferred(1),

                     deprecated(2),

                     invalid(3),

                     inaccessible(4),

                     unknown(5)   -- status can not be determined
                                  -- for some reason.
                    }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
              "Address status.  The preferred(1) state indicates
              that this is a valid address that can appear as
              the destination or source address of a packet.
              The deprecated(2) state indicates that this is
              a valid but deprecated address that should no longer
              be used as a source address in new communications,
              but packets addressed to such an address are
              processed as expected. The invalid(3) state indicates
              that this is not valid address which should not 
              appear as the destination or source address of
              a packet. The inaccessible(4) state indicates that
              the address is not accessible because the interface
              to which this address is assigned is not operational."
            ::= { ipv6PrivAddrEntry 5 }


         -- IPv6 Routing objects

         ipv6PrivRouteNumber OBJECT-TYPE
             SYNTAX      Gauge32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "The number of current ipv6PrivRouteTable entries.
               This is primarily to avoid having to read
               the table in order to determine this number."
             ::= { ipv6PrivMIBObjects 9 }

         ipv6PrivDiscardedRoutes OBJECT-TYPE
             SYNTAX      Counter32
             MAX-ACCESS  read-only
             STATUS      current
             DESCRIPTION
               "The number of routing entries which were chosen
                to be discarded even though they are valid.  One
                possible reason for discarding such an entry could
                be to free-up buffer space for other routing
                entries."
             ::= { ipv6PrivMIBObjects 10 }


         -- IPv6 Routing table

         ipv6PrivRouteTable OBJECT-TYPE
             SYNTAX     SEQUENCE OF Ipv6PrivRouteEntry
             MAX-ACCESS not-accessible
             STATUS     current
             DESCRIPTION
               "IPv6 Routing table. This table contains
               an entry for each valid IPv6 unicast route
               that can be used for packet forwarding
               determination."
             ::= { ipv6PrivMIBObjects 11 }

         ipv6PrivRouteEntry OBJECT-TYPE
             SYNTAX     Ipv6PrivRouteEntry
             MAX-ACCESS not-accessible
             STATUS     current
             DESCRIPTION
                     "A routing entry."   
             INDEX   {  
             		   ipv6PrivRouteVrid,		
             		   ipv6PrivRouteDest,
                       ipv6PrivRoutePfxLength,
                       ipv6PrivRouteIndex,
					   ipv6PrivRouteIndexNextHop}
             ::= { ipv6PrivRouteTable 1 }

         Ipv6PrivRouteEntry ::= SEQUENCE {   
         		 ipv6PrivRouteVrid	         INTEGER,
                 ipv6PrivRouteDest           Ipv6Address,
                 ipv6PrivRoutePfxLength      INTEGER,
                 ipv6PrivRouteIndex          Unsigned32,
				 ipv6PrivRouteIndexNextHop	 Ipv6Address,
                 ipv6PrivRouteIfIndex        Ipv6IfIndexOrZero,
                 ipv6PrivRouteNextHop        Ipv6Address,
                 ipv6PrivRouteType           INTEGER,
                 ipv6PrivRouteProtocol       INTEGER,
                 ipv6PrivRoutePolicy         Integer32,
                 ipv6PrivRouteAge            Unsigned32,
                 ipv6PrivRouteNextHopRDI     Unsigned32,
                 ipv6PrivRouteMetric         Unsigned32,
                 ipv6PrivRouteWeight         Unsigned32,
                 ipv6PrivRouteInfo           RowPointer,
                 ipv6PrivRouteValid          TruthValue
             }
         ipv6PrivRouteVrid   OBJECT-TYPE
             SYNTAX     INTEGER
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
               "the vpn instance id."
             ::= { ipv6PrivRouteEntry 1 }

         
         ipv6PrivRouteDest OBJECT-TYPE
             SYNTAX     Ipv6Address
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
               "The destination IPv6 address of this route.
               This object may not take a Multicast address
               value."
             ::= { ipv6PrivRouteEntry 2 }

         ipv6PrivRoutePfxLength OBJECT-TYPE
             SYNTAX     INTEGER(0..128)
             UNITS      "bits"
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
               "Indicates the prefix length of the destination
               address."
             ::= { ipv6PrivRouteEntry 3 }

         ipv6PrivRouteIndex OBJECT-TYPE
             SYNTAX     Unsigned32
             MAX-ACCESS read-only 
             STATUS     current
             DESCRIPTION
               "The value which uniquely identifies the route
               among the routes to the same network layer
               destination.  The way this value is chosen is
               implementation specific but it must be unique for
               ipv6PrivRouteDest/ipv6PrivRoutePfxLength pair and remain
               constant for the life of the route."
             ::= { ipv6PrivRouteEntry 4 }
		
		 ipv6PrivRouteIndexNextHop OBJECT-TYPE
             SYNTAX     Ipv6Address
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
               "The next IPv6 address of this route."
             ::= { ipv6PrivRouteEntry 5 }
			 
         ipv6PrivRouteIfIndex OBJECT-TYPE
             SYNTAX     Ipv6IfIndexOrZero
             MAX-ACCESS read-write
             STATUS     current
             DESCRIPTION
               "The index value which uniquely identifies the local
               interface through which the next hop of this
               route should be reached.  The interface identified
               by a particular value of this index is the same
               interface as identified by the same value of
               ipv6PrivIfIndex.  For routes of the discard type this
               value can be zero."
             ::= { ipv6PrivRouteEntry 6 }

         ipv6PrivRouteNextHop OBJECT-TYPE
             SYNTAX     Ipv6Address
             MAX-ACCESS read-write
             STATUS     current
             DESCRIPTION
               "On remote routes, the address of the next
               system en route;  otherwise, ::0
               ('00000000000000000000000000000000'H in ASN.1
               string representation)."
             ::= { ipv6PrivRouteEntry 7 }

         ipv6PrivRouteType OBJECT-TYPE
             SYNTAX     INTEGER {
                other(1),     -- none of the following

                              -- an route indicating that
                              -- packets to destinations
                              -- matching this route are
                discard(2),   -- to be discarded

                              -- route to directly
                local(3),     -- connected (sub-)network

                              -- route to a remote
                remote(4)     -- destination

             }
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
                "The type of route. Note that 'local(3)' refers
                to a route for which the next hop is the final
                destination; 'remote(4)' refers to a route for
                which  the  next  hop is not the final
                destination; 'discard(2)' refers to a route
                indicating that packets to destinations matching
                this route are to be discarded (sometimes called
                black-hole route)."
             ::= { ipv6PrivRouteEntry 8}

         ipv6PrivRouteProtocol OBJECT-TYPE
             SYNTAX     INTEGER {
               other(1),   -- none of the following

                           -- non-protocol information,
                           -- e.g., manually configured
               local(2),   -- entries

               netmgmt(3), -- static route

                           -- obtained via Neighbor
                           -- Discovery protocol,
               ndisc(4),   -- e.g., result of Redirect

                           -- the following are all
                           -- dynamic routing protocols
               rip(5),     -- RIPng
               ospf(6),    -- Open Shortest Path First
               bgp(7),     -- Border Gateway Protocol
               idrp(8),    -- InterDomain Routing Protocol
               igrp(9)     -- InterGateway Routing Protocol
             }
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
               "The routing mechanism via which this route was
               learned."
             ::= { ipv6PrivRouteEntry 9 }

         ipv6PrivRoutePolicy OBJECT-TYPE
             SYNTAX     Integer32
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
              "The general set of conditions that would cause the
              selection of one multipath route (set of next hops
              for a given destination) is referred to as 'policy'.
              Unless the mechanism indicated by ipv6PrivRouteProtocol
              specified otherwise, the policy specifier is the
              8-bit Traffic Class field of the IPv6 packet header
              that is zero extended at the left to a 32-bit value.

              Protocols defining 'policy' otherwise must either
              define a set of values which are valid for
              this object or must implement an integer-
              instanced  policy table for which this object's
              value acts as an index."
             ::= { ipv6PrivRouteEntry 10}

         ipv6PrivRouteAge OBJECT-TYPE
             SYNTAX     Unsigned32
             UNITS      "seconds"
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
                "The number of seconds since this route was last
                updated or otherwise determined to be correct.
                Note that no semantics of `too old' can be implied
                except through knowledge of the routing protocol
                by which the route was learned."
             ::= { ipv6PrivRouteEntry 11 }

         ipv6PrivRouteNextHopRDI OBJECT-TYPE
             SYNTAX     Unsigned32
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
                "The Routing Domain ID of the Next Hop.
                The  semantics of this object are determined by
                the routing-protocol specified in  the  route's
                ipv6PrivRouteProtocol value.   When  this object is
                unknown or not relevant its value should be set
                to zero."
             ::= { ipv6PrivRouteEntry 12 }

         ipv6PrivRouteMetric OBJECT-TYPE
             SYNTAX     Unsigned32
             MAX-ACCESS read-write
             STATUS     current
             DESCRIPTION             
                "The routing metric for this route. The
                semantics of this metric are determined by the
                routing protocol specified in the route's
                ipv6PrivRouteProtocol value.  When this is unknown
                or not relevant to the protocol indicated by
                ipv6PrivRouteProtocol, the object value should be
                set to its maximum value (4,294,967,295)."
             ::= { ipv6PrivRouteEntry 13 }

         ipv6PrivRouteWeight OBJECT-TYPE
             SYNTAX     Unsigned32
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
                "The system internal weight value for this route.
                The semantics of this value are determined by
                the implementation specific rules. Generally,
                within routes with the same ipv6PrivRoutePolicy value,
                the lower the weight value the more preferred is
                the route."
             ::= { ipv6PrivRouteEntry 14 }

         ipv6PrivRouteInfo OBJECT-TYPE
             SYNTAX     RowPointer
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
                "A reference to MIB definitions specific to the
                particular routing protocol which is responsible
                for this route, as determined by the  value
                specified  in the route's ipv6PrivRouteProto value.
                If this information is not present,  its  value
                should be set to the OBJECT ID { 0 0 },
                which is a syntactically valid object  identifier,
                and any implementation conforming to ASN.1
                and the Basic Encoding Rules must  be  able  to
                generate and recognize this value."
             ::= { ipv6PrivRouteEntry 15 }

         ipv6PrivRouteValid OBJECT-TYPE
             SYNTAX     TruthValue
             MAX-ACCESS read-write
             STATUS     current
             DESCRIPTION
                "Setting this object to the value 'false(2)' has
                the effect of invalidating the corresponding entry
                in the ipv6PrivRouteTable object.  That is, it
                effectively disassociates the destination
                identified with said entry from the route
                identified with said entry.  It is an
                implementation-specific matter as to whether the
                agent removes an invalidated entry from the table.
                Accordingly, management stations must be prepared
                to receive tabular information from agents that
                corresponds to entries not currently in use.
                Proper interpretation of such entries requires
                examination of the relevant ipv6PrivRouteValid
                object."
             DEFVAL  { true }
             ::= { ipv6PrivRouteEntry 16 }


         -- IPv6 Address Translation table

         ipv6PrivNetToMediaTable OBJECT-TYPE
             SYNTAX      SEQUENCE OF Ipv6PrivNetToMediaEntry
             MAX-ACCESS  not-accessible
             STATUS      current
             DESCRIPTION
               "The IPv6 Address Translation table used for
               mapping from IPv6 addresses to physical addresses.

               The IPv6 address translation table contain the
               Ipv6Address to `physical' address equivalencies.
               Some interfaces do not use translation tables
               for determining address equivalencies; if all
               interfaces are of this type, then the Address
               Translation table is empty, i.e., has zero
               entries."
             ::= { ipv6PrivMIBObjects 12 }

         ipv6PrivNetToMediaEntry OBJECT-TYPE
             SYNTAX     Ipv6PrivNetToMediaEntry
             MAX-ACCESS not-accessible
             STATUS     current
             DESCRIPTION
               "Each entry contains one IPv6 address to `physical'
               address equivalence."
             INDEX   { ipv6PrivIfIndex,
                       ipv6PrivNetToMediaNetAddress }
             ::= { ipv6PrivNetToMediaTable 1 }

         Ipv6PrivNetToMediaEntry ::= SEQUENCE {
                 ipv6PrivNetToMediaNetAddress
                     Ipv6Address,
                 ipv6PrivNetToMediaPhysAddress
                     PhysAddress,
                 ipv6PrivNetToMediaType
                     INTEGER,
                 ipv6PrivIfNetToMediaState
                     INTEGER,
                 ipv6PrivIfNetToMediaLastUpdated
                     TimeStamp,
                 ipv6PrivNetToMediaValid
                     TruthValue,
                 ipv6PrivNetToMediaPhyPort
                 	 INTEGER,
                 ipv6PrivNetToMediaAgingTime
                 	 INTEGER	 
             }

         ipv6PrivNetToMediaNetAddress OBJECT-TYPE
             SYNTAX     Ipv6Address
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
                "The IPv6 Address corresponding to
                the media-dependent `physical' address."
             ::= { ipv6PrivNetToMediaEntry 1 }

         ipv6PrivNetToMediaPhysAddress OBJECT-TYPE
             SYNTAX     PhysAddress
             MAX-ACCESS read-write
             STATUS     current
             DESCRIPTION
               "The media-dependent `physical' address."
             ::= { ipv6PrivNetToMediaEntry 2 }

         ipv6PrivNetToMediaType OBJECT-TYPE
             SYNTAX     INTEGER {
                         other(1),    -- none of the following
                         dynamic(2),  -- dynamically resolved
                         static(3),   -- statically configured
                         local(4)     -- local interface
                        }
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
                     "The type of the mapping. The 'dynamic(2)' type
                     indicates that the IPv6 address to physical
                     addresses mapping has been dynamically
                     resolved using the IPv6 Neighbor Discovery
                     protocol. The static(3)' types indicates that
                     the mapping has been statically configured.
                     The local(4) indicates that the mapping is
                     provided for an entity's own interface address."
             ::= { ipv6PrivNetToMediaEntry 3 }
             
         ipv6PrivIfNetToMediaState OBJECT-TYPE
            SYNTAX      INTEGER {
                     reachable(1), -- confirmed reachability

                     stale(2),     -- unconfirmed reachability

                     delay(3),     -- waiting for reachability
                                   -- confirmation before entering
                                   -- the probe state

                     probe(4),     -- actively probing

                     invalid(5),   -- an invalidated mapping

                     unknown(6)    -- state can not be determined
                                   -- for some reason.
                    }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The Neighbor Unreachability Detection [8] state
                for the interface when the address mapping in
                this entry is used."
            ::= { ipv6PrivNetToMediaEntry 4 }

        ipv6PrivIfNetToMediaLastUpdated OBJECT-TYPE
            SYNTAX      TimeStamp
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "The value of sysUpTime at the time this entry
                was last updated.  If this entry was updated prior
                to the last re-initialization of the local network
                management subsystem, then this object contains
                a zero value."
            ::= { ipv6PrivNetToMediaEntry 5 }

         ipv6PrivNetToMediaValid OBJECT-TYPE
             SYNTAX     TruthValue
             MAX-ACCESS read-write
             STATUS     current
             DESCRIPTION
              "Setting this object to the value 'false(2)' has
              the effect of invalidating the corresponding entry
              in the ipv6PrivNetToMediaTable.  That is, it effectively
              disassociates the interface identified with said
              entry from the mapping identified with said entry.
              It is an implementation-specific matter as to
              whether the agent removes an invalidated entry
              from the table.  Accordingly, management stations
              must be prepared to receive tabular information
              from agents that corresponds to entries not
              currently in use.  Proper interpretation of such
              entries requires examination of the relevant
              ipv6PrivNetToMediaValid object."
             DEFVAL  { true }
             ::= { ipv6PrivNetToMediaEntry 6 }
             
          ipv6PrivNetToMediaPhyPort OBJECT-TYPE
             SYNTAX     INTEGER
             MAX-ACCESS read-write
             STATUS     current
             DESCRIPTION
              "The Phy Port of ipv6 address."
             ::= { ipv6PrivNetToMediaEntry 7 }    
             
         ipv6PrivNetToMediaAgingTime OBJECT-TYPE
             SYNTAX     INTEGER
             MAX-ACCESS read-only
             STATUS     current
             DESCRIPTION
              "The ipv6 neighbor aging time."
             ::= { ipv6PrivNetToMediaEntry 8 }   
                  
ipv6PrivTcpConnTable OBJECT-TYPE
   SYNTAX      SEQUENCE OF Ipv6PrivTcpConnEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
        "A table containing TCP connection-specific information,
         for only those connections whose endpoints are IPv6 addresses."
   ::= { ipv6PrivMIBObjects 13 }

ipv6PrivTcpConnEntry OBJECT-TYPE
   SYNTAX      Ipv6PrivTcpConnEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
        "A conceptual row of the ipv6PrivTcpConnTable containing
         information about a particular current TCP connection.
         Each row of this table is transient, in that it ceases to
         exist when (or soon after) the connection makes the transition
         to the CLOSED state.

         Note that conceptual rows in this table require an additional
         index object compared to tcpConnTable, since IPv6 addresses
         are not guaranteed to be unique on the managed node."
   INDEX   { ipv6PrivTcpConnLocalAddress,
             ipv6PrivTcpConnLocalPort,
             ipv6PrivTcpConnRemAddress,
             ipv6PrivTcpConnRemPort,
             ipv6PrivTcpConnIfIndex }
   ::= { ipv6PrivTcpConnTable 1 }

Ipv6PrivTcpConnEntry ::=
   SEQUENCE { ipv6PrivTcpConnLocalAddress    Ipv6Address,
              ipv6PrivTcpConnLocalPort       INTEGER,
              ipv6PrivTcpConnRemAddress      Ipv6Address,
              ipv6PrivTcpConnRemPort         INTEGER,
              ipv6PrivTcpConnIfIndex         Ipv6IfIndexOrZero,
              ipv6PrivTcpConnState           INTEGER }

ipv6PrivTcpConnLocalAddress OBJECT-TYPE
   SYNTAX     Ipv6Address
   MAX-ACCESS read-only
   STATUS     current
   DESCRIPTION
        "The local IPv6 address for this TCP connection. In
         the case of a connection in the listen state which
         is willing to accept connections for any IPv6
         address associated with the managed node, the value
         ::0 is used."
   ::= { ipv6PrivTcpConnEntry 1 }

ipv6PrivTcpConnLocalPort OBJECT-TYPE
   SYNTAX     INTEGER (0..65535)
   MAX-ACCESS read-only
   STATUS     current
   DESCRIPTION
        "The local port number for this TCP connection."
   ::= { ipv6PrivTcpConnEntry 2 }

ipv6PrivTcpConnRemAddress OBJECT-TYPE
   SYNTAX     Ipv6Address
   MAX-ACCESS read-only
   STATUS     current
   DESCRIPTION
        "The remote IPv6 address for this TCP connection."
   ::= { ipv6PrivTcpConnEntry 3 }

ipv6PrivTcpConnRemPort OBJECT-TYPE
   SYNTAX     INTEGER (0..65535)
   MAX-ACCESS read-only
   STATUS     current
   DESCRIPTION
        "The remote port number for this TCP connection."
   ::= { ipv6PrivTcpConnEntry 4 }

ipv6PrivTcpConnIfIndex OBJECT-TYPE
   SYNTAX     Ipv6IfIndexOrZero
   MAX-ACCESS read-only
   STATUS     current
   DESCRIPTION
        "An index object used to disambiguate conceptual rows in
         the table, since the connection 4-tuple may not be unique.
         If the connection's remote address (ipv6PrivTcpConnRemAddress)
         is a link-local address and the connection's local address
         (ipv6PrivTcpConnLocalAddress) is not a link-local address, this
         object identifies a local interface on the same link as
         the connection's remote link-local address.
         Otherwise, this object identifies the local interface that
         is associated with the ipv6PrivTcpConnLocalAddress for this
         TCP connection.  If such a local interface cannot be determined,
         this object should take on the value 0.  (A possible example
         of this would be if the value of ipv6PrivTcpConnLocalAddress is ::0.)
         The interface identified by a particular non-0 value of this
         index is the same interface as identified by the same value
         of ipv6PrivIfIndex.
         The value of this object must remain constant during the life
         of the TCP connection."
   ::= { ipv6PrivTcpConnEntry 5 }

ipv6PrivTcpConnState OBJECT-TYPE
   SYNTAX     INTEGER {
        closed(1),
        listen(2),
        synSent(3),
        synReceived(4),
        established(5),
        finWait1(6),
        finWait2(7),
        closeWait(8),
        lastAck(9),
        closing(10),
        timeWait(11),
        deleteTCB(12) }
   MAX-ACCESS read-write
   STATUS     current
   DESCRIPTION
        "The state of this TCP connection.

         The only value which may be set by a management station is
         deleteTCB(12).  Accordingly, it is appropriate for an agent
         to return an error response (`badValue' for SNMPv1, 'wrongValue'
         for SNMPv2) if a management station attempts to set this
         object to any other value.

         If a management station sets this object to the value
         deleteTCB(12), then this has the effect of deleting the TCB
         (as defined in RFC 793) of the corresponding connection on
         the managed node, resulting in immediate termination of the
         connection.

         As an implementation-specific option, a RST segment may be
         sent from the managed node to the other TCP endpoint (note
         however that RST segments are not sent reliably)."
   ::= { ipv6PrivTcpConnEntry 6 }

ipv6PrivUdpTable OBJECT-TYPE
   SYNTAX      SEQUENCE OF Ipv6PrivUdpEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
        "A table containing UDP listener information for
         UDP/IPv6 endpoints."
   ::= {ipv6PrivMIBObjects 14}

ipv6PrivUdpEntry OBJECT-TYPE
   SYNTAX      Ipv6PrivUdpEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
        "Information about a particular current UDP listener.

         Note that conceptual rows in this table require an
         additional index object compared to udpTable, since
         IPv6 addresses are not guaranteed to be unique on the
         managed node."
   INDEX   { ipv6PrivUdpLocalAddress,
             ipv6PrivUdpLocalPort,
             ipv6PrivUdpIfIndex }
   ::= { ipv6PrivUdpTable 1 }

Ipv6PrivUdpEntry ::= SEQUENCE {
   ipv6PrivUdpLocalAddress    Ipv6Address,
   ipv6PrivUdpLocalPort       INTEGER,
   ipv6PrivUdpIfIndex         Ipv6IfIndexOrZero }

ipv6PrivUdpLocalAddress OBJECT-TYPE
   SYNTAX       Ipv6Address
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
        "The local IPv6 address for this UDP listener.
         In the case of a UDP listener which is willing
         to accept datagrams for any IPv6 address
         associated with the managed node, the value ::0
         is used."
   ::= { ipv6PrivUdpEntry 1 }

ipv6PrivUdpLocalPort OBJECT-TYPE
    SYNTAX     INTEGER (0..65535)
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The local port number for this UDP listener."
    ::= { ipv6PrivUdpEntry 2 }

ipv6PrivUdpIfIndex OBJECT-TYPE
   SYNTAX     Ipv6IfIndexOrZero
   MAX-ACCESS   read-only
   STATUS     current
   DESCRIPTION
        "An index object used to disambiguate conceptual rows in
         the table, since the ipv6PrivUdpLocalAddress/ipv6PrivUdpLocalPort
         pair may not be unique.

         This object identifies the local interface that is
         associated with ipv6PrivUdpLocalAddress for this UDP listener.
         If such a local interface cannot be determined, this object
         should take on the value 0.  (A possible example of this
         would be if the value of ipv6PrivUdpLocalAddress is ::0.)

         The interface identified by a particular non-0 value of
         this index is the same interface as identified by the same
         value of ipv6PrivIfIndex.

         The value of this object must remain constant during
         the life of this UDP endpoint."
   ::= { ipv6PrivUdpEntry 3 }
   
ipv6PrivNeighborStatic   OBJECT-TYPE
             SYNTAX      INTEGER
             MAX-ACCESS  read-only
             STATUS     current
             DESCRIPTION
                "The static ipv6 neighbor number"
              ::= { ipv6PrivMIBObjects 15 }
              
   ipv6PrivNeighborDynamic   OBJECT-TYPE
             SYNTAX      INTEGER
             MAX-ACCESS  read-only
             STATUS     current
             DESCRIPTION
                "The Dynamic ipv6 neighbor number"
              ::= { ipv6PrivMIBObjects 16 }           
    
   ipv6PrivNeighborAge   OBJECT-TYPE
             SYNTAX      INTEGER
             MAX-ACCESS  read-write
             STATUS     current
             DESCRIPTION
                "The neighbor aging time"
