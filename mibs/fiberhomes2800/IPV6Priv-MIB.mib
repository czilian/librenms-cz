        -- compliance statements
        ipv6PrivCompliance MODULE-COMPLIANCE
            STATUS  current
            DESCRIPTION
              "The compliance statement for SNMPv2 entities which
              implement ipv6Priv MIB."
            MODULE  -- this module
                MANDATORY-GROUPS { ipv6PrivGeneralGroup,
                                   ipv6PrivNotificationGroup }
                  OBJECT    ipv6PrivForwarding
                    MIN-ACCESS  read-only
                    DESCRIPTION
                       "An agent is not required to provide write
                        access to this object"
                  OBJECT    ipv6PrivDefaultHopLimit
                    MIN-ACCESS  read-only
                    DESCRIPTION
                       "An agent is not required to provide write
                        access to this object"
                  OBJECT    ipv6PrivIfDescr
