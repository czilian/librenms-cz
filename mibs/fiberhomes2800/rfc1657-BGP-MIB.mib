                }

                bgp4PathAttrProtocol  OBJECT-TYPE
                    SYNTAX     INTEGER{  
                    		other(1),
							local(2),
							netmgmt(3),
							icmp(4),
							egp(5),
							ggp(6),
							hello(7),
							rip(8),
							isis(9),
							esis(10),
							ciscoIgrp(11),
							bbnSpfIgp(12),
							ospf(13),
							bgp(14)	}

                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "Protocol type:
                            other           	1
							local           	2
							netmgmt         	3
							icmp            	4
							egp             	5
							ggp             	6
							hello           	7
							rip             	8
							is_is           	9
							es_is				10
							ciscoIgrp			11
							bbnSpfIgp			12
							ospf				13
							bgp     			14"
                    ::= { bgp4PathAttrEntry 1 }
                          
                   bgp4PathAttrIpAddrType OBJECT-TYPE
	                    SYNTAX     InetAddressType                                                                                                      MAX-ACCESS read-only
	                    STATUS     current
	                    DESCRIPTION
	                            " ip address type in
                     			the Network Layer Reachability
                     			Information field."
	                    ::= { bgp4PathAttrEntry 2 }
  
                    bgp4PathAttrIpAddrPrefix OBJECT-TYPE
	                    SYNTAX     InetAddress 
	                    MAX-ACCESS read-only
	                    STATUS     current
	                    DESCRIPTION
	                            " An IP address prefix in the Network Layer
                     Reachability Information field.  This object is an IP address containing the prefix with
                     length specified by bgp4PathAttrIpAddrPrefixLen.Any bits beyond the length specified by
                     bgp4PathAttrIpAddrPrefixLen are zeroed."
	                    ::= { bgp4PathAttrEntry 3 }
                         
                   bgp4PathAttrIpAddrPrefixLen   OBJECT-TYPE
	                    SYNTAX     INTEGER 
                        MAX-ACCESS read-only
	                    STATUS     current
	                    DESCRIPTION
	                            "Length in bits of the IP address prefix in
                     the Network Layer Reachability
                     Information field."
	                    ::= { bgp4PathAttrEntry 4 }  
	                    
                   bgp4PathAttrPeerAddrType OBJECT-TYPE
	                    SYNTAX     InetAddressType                                                                                                      MAX-ACCESS read-only
	                    STATUS     current
	                    DESCRIPTION
	                            "The IP address type of the peer where the path
                     information was learned"
	                    ::= { bgp4PathAttrEntry 5 }

                      bgp4PathAttrPeer  OBJECT-TYPE
	                    SYNTAX     InetAddress 
	                    MAX-ACCESS read-only
	                    STATUS     current
	                    DESCRIPTION
	                            "The IP address of the peer where the path
                     information was learned"
