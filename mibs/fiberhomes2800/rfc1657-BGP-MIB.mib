 
BGP-MIB DEFINITIONS ::= BEGIN

  IMPORTS
      MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
      IpAddress, Integer32, Counter32, Gauge32,enterprises
           FROM SNMPv2-SMI     
        TEXTUAL-CONVENTION, TruthValue, RowStatus
           FROM SNMPv2-TC     
      mib-2
          FROM RFC1213-MIB
      InetAddress, InetAddressType         
          FROM INET-ADDRESS-MIB 
       wri,wriProtocol	
        FROM WRI-SMI ;   
           
   	bgpStdMIB  MODULE-IDENTITY
       LAST-UPDATED  "201104060000Z" 
       ORGANIZATION  "FiberHome Networks Systems, Inc"
       CONTACT-INFO  "http://www.fhn.com.cn"   
       DESCRIPTION   "reference to rfc1657."
          ::= { wriProtocol 242 }

     	   
                bgp OBJECT IDENTIFIER  ::= { bgpStdMIB 1 }

                bgpVersion OBJECT-TYPE
                    SYNTAX     INTEGER
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "version number."
                    ::= { bgp 1 }

                bgpLocalAs OBJECT-TYPE
                    SYNTAX     INTEGER (0..65535)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The local autonomous system number."
                    ::= { bgp 2 }

                bgpIdentifier OBJECT-TYPE
                    SYNTAX     InetAddress
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The BGP Identifier of local system."
                    ::= { bgp 3 }

                            
                bgpNexthopSelf OBJECT-TYPE
                    SYNTAX     INTEGER {
                                        disable(0),
                                        enable(1)
                               }
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "is bgp support set nexthop as self"
                    ::= { bgp 4 } 
                                                            
                bgpClusterId OBJECT-TYPE
                    SYNTAX   InetAddress
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "When Bgp act as a route cluster,it need set cluster id"
                    ::= { bgp 5 } 
                                                                    
                bgpCummunityAction OBJECT-TYPE
                    SYNTAX     INTEGER {
                                        noaction(0),
                                        add(1),
                                        replace(2),
                                        none(3)
                                       }
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "describe how to process community"
                    ::= { bgp 6 }  
                    
                 bgpCummunity OBJECT-TYPE
                    SYNTAX     INTEGER 
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "describe bgp community"
                    ::= { bgp 7 }   
                        
                 bgpConfederationId OBJECT-TYPE
                    SYNTAX   INTEGER
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "ConfederationId"
                    ::= { bgp 8 }  
                    
                bgpTrapSupport OBJECT-TYPE
                    SYNTAX   INTEGER {
                                       disable(0),
                                       enable(1)
                                     }
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "configure if bgp can running trap"
                    ::= { bgp 9 }
                
                bgpDefaultLocalPref OBJECT-TYPE
                    SYNTAX     INTEGER
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The DefaultLocalPref"
                    ::= { bgp 10 } 
                    
                  bgpSyncStatus OBJECT-TYPE
                    SYNTAX     INTEGER {
                                        disable(2),
                                        enable(1)
                                       }
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The DefaultLocalPref"
                    ::= { bgp 11 }   
                    
                bgpLocal4BytesAs OBJECT-TYPE
                    SYNTAX     INTEGER 
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The local autonomous system number,extend to 4bytes"
                    ::= { bgp 12 }
                
                 bgpServerState OBJECT-TYPE
                    SYNTAX     INTEGER{disable(0),enable(1)} 
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "bgp server state"
                    ::= { bgp 13 }    
                    
				bgpClinetState OBJECT-TYPE
                    SYNTAX     INTEGER{disable(0),enable(1)} 
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "bgp clinet state"
                    ::= { bgp 14 }    
                    
    			bgpUpdateSize OBJECT-TYPE
                    SYNTAX     INTEGER(0..4097) 
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "bgp update size"
                    ::= { bgp 15 }  
                                      
   				bgpTaskPriority OBJECT-TYPE
                    SYNTAX     INTEGER(50..250)
                    MAX-ACCESS read-write      
                    STATUS     current
                    DESCRIPTION
                            "bgp task priority"
                    ::= { bgp 16 }      
                                  
    			bgpMaxStackSize OBJECT-TYPE
                    SYNTAX     INTEGER
                    MAX-ACCESS read-write      
                    STATUS     current
                    DESCRIPTION
                            "bgp max stack size"
                    ::= { bgp 17 }      
                    
   				bgpMaxPeer     OBJECT-TYPE
                    SYNTAX     INTEGER(1..400000)
                    MAX-ACCESS read-write      
                    STATUS     current
                    DESCRIPTION
                            "bgp max peer"
                    ::= { bgp 18}
                      
    			bgpMaxRoute     OBJECT-TYPE
                    SYNTAX     INTEGER(1..400000)
                    MAX-ACCESS read-write      
                    STATUS     current
                    DESCRIPTION
                            "bgp max route"
                    ::= { bgp 19 }        
                    
     			bgpMaxPath     OBJECT-TYPE
                    SYNTAX     INTEGER(1..400000)
                    MAX-ACCESS read-write      
                    STATUS     current
                    DESCRIPTION
                            "bgp max path"
                    ::= { bgp 20 }    
                                                                       
               bgpDefaultLocalMed OBJECT-TYPE
                    SYNTAX     INTEGER
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The DefaultLocal med"
                    ::= { bgp 21 } 
                
               bgpProtocolRouteCnt OBJECT-TYPE
                    SYNTAX     OCTET STRING 
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The route number which is leaned by other protocol"
                    ::= { bgp 22 } 
                    
              bgpGrEnable OBJECT-TYPE
                    SYNTAX   INTEGER{disbale(0),enable(1)}   
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The state of graceful restart"
                    ::= { bgp 23 }  
                    
               bgpGrTimer OBJECT-TYPE
                    SYNTAX   INTEGER(3..600)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The timer of graceful restart,in second"
                    ::= { bgp 24 }      
                                    
               bgpGrSelectionDeferralTimer OBJECT-TYPE
                    SYNTAX   INTEGER(3..3000)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The timer of graceful restart Selection Deferral,in second"
                    ::= { bgp 25 }                                                                      
               
               bgpResetState OBJECT-TYPE
                    SYNTAX   INTEGER
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "reset bgp process"
                    ::= { bgp 26 }    
                    
               bgpDampState OBJECT-TYPE
                    SYNTAX   INTEGER{disable(0),enable(1)}
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "route damping state"
                    ::= { bgp 49}           
               
               bgpDampPenaltyHalfTime   OBJECT-TYPE
                    SYNTAX   INTEGER(60..2700)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "Half-life time (in seconds) for the penalty when reachable"
                    ::= { bgp 50 }                                                    
                    
               bgpDampPenaltyLimit   OBJECT-TYPE
                    SYNTAX   INTEGER(1001..20000)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            " Ceiling of the penalty for route flapping"
                    ::= { bgp 51 }      
               
               bgpDampPenaltySuppress   OBJECT-TYPE
                    SYNTAX   INTEGER(1..20000)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "Value to start suppressing a route"
                    ::= { bgp 52 }   
                    
                bgpDampPenaltyReuse   OBJECT-TYPE
                    SYNTAX   INTEGER(1..20000)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "Value to start reusing a route"
                    ::= { bgp 53 }   
   
              
              	bgpBackupPathState   OBJECT-TYPE
                    SYNTAX   INTEGER{disable(0),enable(1)}
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "Fast switching backup route "
                    ::= { bgp 56 }  
                             
                -- BGP Peer table.  This table contains, one entry per
                -- BGP peer, information about the BGP peer.

                bgpPeerTable OBJECT-TYPE
                    SYNTAX     SEQUENCE OF BgpPeerEntry
                    MAX-ACCESS not-accessible
                    STATUS     current
                    DESCRIPTION
                            "BGP peer table.  This table contains,
                            one entry per BGP peer, information about
                            the connections with BGP peers."
                    ::= { bgp 27 }

                bgpPeerEntry OBJECT-TYPE
                    SYNTAX     BgpPeerEntry
                    MAX-ACCESS not-accessible
                    STATUS     current
                    DESCRIPTION
                            "Entry containing information about the
                            connection with a BGP peer."
                    INDEX { bgpPeerVrid,bgpPeerAddrType,bgpPeerRemoteAddr}
                    
                    ::= { bgpPeerTable 1 }

                BgpPeerEntry ::= SEQUENCE {
                		bgpPeerVrid 
                			INTEGER,
                        bgpPeerAddrType 
                            InetAddressType,
        				bgpPeerRemoteAddr
                            InetAddress,
                       bgpPeerIdentifier
                            InetAddress,
                        bgpPeerState
                            INTEGER,
                        bgpPeerAdminStatus
                            INTEGER,
                        bgpPeerNegotiatedVersion
                            Integer32, 
                        bgpPeerLocalAddrType 
                            InetAddressType,    
                        bgpPeerLocalAddr
                            InetAddress,
                        bgpPeerLocalPort
                            INTEGER,
                        bgpPeerRemotePort
                            INTEGER,
                        bgpPeerRemoteAs
                            INTEGER,
                        bgpPeerInUpdates
                            Counter32,
                        bgpPeerOutUpdates
                            Counter32,
                        bgpPeerInTotalMessages
                            Counter32,
                        bgpPeerOutTotalMessages
                            Counter32,
                        bgpPeerLastError
                            OCTET STRING,
                        bgpPeerFsmEstablishedTransitions
                            Counter32,
                        bgpPeerFsmEstablishedTime
                            Gauge32,
                        bgpPeerConnectRetryInterval
                            INTEGER,
                        bgpPeerHoldTime
                            INTEGER,
                        bgpPeerKeepAlive
                            INTEGER, 
                        bgpPeerHoldTimeConfigured
                    		INTEGER,
               			bgpPeerKeepAliveConfigured
                    		INTEGER,    
                        bgpPeerMinASOriginationInterval
                            INTEGER,
                        bgpPeerMinRouteAdvertisementInterval
                            INTEGER,
                        bgpPeerInUpdateElapsedTime
                            Gauge32,       
                        bgpPeerPasswordSupport 
                            INTEGER,    
                        bgpPeerPassword 
                            OCTET STRING,    
                        bgpPeerRtReflectorState 
                            INTEGER,        
                        bgpPeerAttachedVrf 
                            OCTET STRING,   
                        bgpPeerSendCommunity 
                            INTEGER,
                        bgpPeerRawStatus 
                            INTEGER,
                        bgpPeerUpdateSource
                            InetAddress,
                        
--new peer attribute S ddp add 20051010
                 
						bgpPeer4BytesAsNumber INTEGER,
						bgpPeer4BytesAsSupport INTEGER,
						bgpPeerLocalAfiFlag INTEGER ,
						bgpPeerRemoteAfiFlag INTEGER ,
						bgpPeerLocalRefresh INTEGER,
						bgpPeerRemoteRefresh INTEGER,
						bgpPeerGrRemoteSupport INTEGER ,
						bgpPeerGrRemoteResetBit INTEGER,
						bgpPeerGrRemoteResetTime INTEGER,
						bgpPeerGrLocalRole INTEGER     ,
						bgpPeerEbgpMtHop INTEGER ,
            			bgpPeerCaseReason INTEGER ,
            			bgpPeerMaxPrefix INTEGER ,
            			bgpPeerResourceLack INTEGER ,
                        bgpPeerRejectConnect INTEGER ,
                        bgpPeerEbgp INTEGER,
                        bgpPeerRtRefresh INTEGER,
                        bgpPeerGrRemoteResetAf INTEGER,
                        bgpPeerRestart  INTEGER,
                        bgpPeerUnConfig  INTEGER,
                        bgpPeerIpv6Ce    INTEGER,
                        bgpPeerSendLable INTEGER,
                       	bgpPeerValidTtl   INTEGER,
                       	bgpPeerRouteCnt Counter32, 
                       	bgpPeerNexthopSelf  INTEGER,
                       	bgpPeerBfdState     INTEGER,
                       	bgpPeerPublicAsOnly INTEGER,
                       	bgpPeerSubstitureAs INTEGER,
                       	bgpPeerAsLoopTime   INTEGER,
                       	bgpPeerFakeAs       INTEGER,
                       	bgpPeerUpe          INTEGER                 	 
                        }  
                bgpPeerVrid OBJECT-TYPE
                    SYNTAX     INTEGER
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The vpn instance id."
                    ::= { bgpPeerEntry 1 }
       
                bgpPeerAddrType OBJECT-TYPE
                    SYNTAX     InetAddressType
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The remote IP address type of this entry's BGP
                            peer."
                    ::= { bgpPeerEntry 2 }
                    
                bgpPeerRemoteAddr OBJECT-TYPE
                    SYNTAX     InetAddress
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The remote IP address of this entry's BGP
                            peer."
                    ::= { bgpPeerEntry 3 }
                   
                bgpPeerIdentifier OBJECT-TYPE
                    SYNTAX     InetAddress
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The BGP Identifier of this entry's BGP
                            peer."
                    ::= { bgpPeerEntry 4 }

                bgpPeerState OBJECT-TYPE
                    SYNTAX     INTEGER {
                                        idle(1),
                                        connect(2),
                                        active(3),
                                        opensent(4),
                                        openconfirm(5),
                                        established(6)
                               }
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The BGP peer connection state."
                    ::= { bgpPeerEntry 5 }

                bgpPeerAdminStatus OBJECT-TYPE
                    SYNTAX     INTEGER {
                                        stop(1),
                                        start(2)
                                 }
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The desired state of the BGP connection.
                            A transition from 'stop' to 'start' will
                            cause the BGP Start Event to be generated.
                            A transition from 'start' to 'stop' will
                            cause the BGP Stop Event to be generated.
                            This parameter can be used to restart BGP
                            peer connections.  Care should be used in
                            providing write access to this object
                            without adequate authentication."
                    ::= { bgpPeerEntry 6 }

                bgpPeerNegotiatedVersion OBJECT-TYPE
                    SYNTAX     Integer32
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The negotiated version of BGP running
                            between the two peers."
                    ::= { bgpPeerEntry 7 }
                    
               bgpPeerLocalAddrType  OBJECT-TYPE
                    SYNTAX     InetAddressType
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The local IP address type of this entry's BGP
                            connection."
                    ::= { bgpPeerEntry 8 }

                bgpPeerLocalAddr OBJECT-TYPE
                    SYNTAX     InetAddress
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The local IP address of this entry's BGP
                            connection."
                    ::= { bgpPeerEntry 9 }

                bgpPeerLocalPort OBJECT-TYPE
                    SYNTAX     INTEGER (0..65535)
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The local port for the TCP connection
                            between the BGP peers."
                    ::= { bgpPeerEntry 10 }

                
                bgpPeerRemotePort OBJECT-TYPE
                    SYNTAX     INTEGER (0..65535)
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The remote port for the TCP connection
                            between the BGP peers.  Note that the
                            objects bgpPeerLocalAddr,
                            bgpPeerLocalPort, bgpPeerRemoteAddr and
                            bgpPeerRemotePort provide the appropriate
                            reference to the standard MIB TCP
                            connection table."
                    ::= { bgpPeerEntry 11 }

                bgpPeerRemoteAs OBJECT-TYPE
                    SYNTAX     INTEGER (0..65535)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "The remote autonomous system number."
                    ::= { bgpPeerEntry 12 }

                bgpPeerInUpdates OBJECT-TYPE
                    SYNTAX     Counter32
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The number of BGP UPDATE messages
                            received on this connection.  This object
                            should be initialized to zero (0) when the
                            connection is established."
                    ::= { bgpPeerEntry 13 }

                bgpPeerOutUpdates OBJECT-TYPE
                    SYNTAX     Counter32
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The number of BGP UPDATE messages
                            transmitted on this connection.  This
                            object should be initialized to zero (0)
                            when the connection is established."
                    ::= { bgpPeerEntry 14 }

                bgpPeerInTotalMessages OBJECT-TYPE
                    SYNTAX     Counter32
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The total number of messages received
                            from the remote peer on this connection.
                            This object should be initialized to zero
                            when the connection is established."
                    ::= { bgpPeerEntry 15 }

                bgpPeerOutTotalMessages OBJECT-TYPE
                    SYNTAX     Counter32
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The total number of messages transmitted to
                            the remote peer on this connection.  This
                            object should be initialized to zero when
                            the connection is established."
                    ::= { bgpPeerEntry 16 }

                bgpPeerLastError OBJECT-TYPE
                    SYNTAX     OCTET STRING (SIZE (0..2))   
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The last error code and subcode seen by this
						 	peer on this connection. If no error has
						 	occurred, this field is zero. Otherwise, the
						 	first byte of this two byte OCTET STRING
						 	contains the error code; the second contains
						 	the subcode"
                    ::= { bgpPeerEntry 17 }   
                    
             

                bgpPeerFsmEstablishedTransitions OBJECT-TYPE
                    SYNTAX     Counter32
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "The total number of times the BGP FSM
                            transitioned into the established state."
                    ::= { bgpPeerEntry 18 }

                bgpPeerFsmEstablishedTime OBJECT-TYPE
                    SYNTAX     Gauge32
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "This timer indicates how long (in
                            seconds) this peer has been in the
                            Established state or how long
                            since this peer was last in the
                            Established state.  It is set to zero when
                            a new peer is configured or the router is
                            booted."
                    ::= { bgpPeerEntry 19 }

                bgpPeerConnectRetryInterval OBJECT-TYPE
                    SYNTAX     INTEGER (1..65535)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "Time interval in seconds for the
                            ConnectRetry timer.  The suggested value
                            for this timer is 120 seconds."
                    ::= { bgpPeerEntry 20 }

                bgpPeerHoldTime OBJECT-TYPE
                    SYNTAX     INTEGER  ( 0 | 3..65535 )
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "Time interval in seconds for the Hold
                            Timer established with the peer.  The
                            value of this object is calculated by this
                            BGP speaker by using the smaller of the
                            value in bgpPeerHoldTimeConfigured and the
                            Hold Time received in the OPEN message.
                            This value must be at lease three seconds
                            if it is not zero (0) in which case the
                            Hold Timer has not been established with
                            the peer, or, the value of
                            bgpPeerHoldTimeConfigured is zero (0)."
                    ::= { bgpPeerEntry 21 }

                bgpPeerKeepAlive OBJECT-TYPE
                    SYNTAX     INTEGER ( 0 | 1..21845 )
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "Time interval in seconds for the KeepAlive
                            timer established with the peer.  The value
                            of this object is calculated by this BGP
                            speaker such that, when compared with
                            bgpPeerHoldTime, it has the same
                            proportion as what
                            bgpPeerKeepAliveConfigured has when
                            compared with bgpPeerHoldTimeConfigured.
                            If the value of this object is zero (0),
                            it indicates that the KeepAlive timer has
                            not been established with the peer, or,
                            the value of bgpPeerKeepAliveConfigured is
                            zero (0)."
                    ::= { bgpPeerEntry 22 }

		          bgpPeerHoldTimeConfigured OBJECT-TYPE
		            SYNTAX     INTEGER ( 0 | 3..65535 )
		            UNITS      "seconds"
		            MAX-ACCESS read-write
		            STATUS     current
		            DESCRIPTION
		                    "Time interval (in seconds) for the Hold Time
		                     configured for this BGP speaker with this
		                     peer.  This value is placed in an OPEN
		                     message sent to this peer by this BGP
		                     speaker, and is compared with the Hold
		                     Time field in an OPEN message received
		                     from the peer when determining the Hold
		                     Time (bgpPeerHoldTime) with the peer.
		                     This value must not be less than three
		                     seconds if it is not zero (0).  If it is
		                     zero (0), the Hold Time is NOT to be
		                     established with the peer.  The suggested
		                     value for this timer is 90 seconds."
		            REFERENCE
		                    "RFC 4271, Section 4.2.
		                     RFC 4271, Section 10."
		            ::= { bgpPeerEntry 23 }
            
            	bgpPeerKeepAliveConfigured OBJECT-TYPE             	
	            SYNTAX     INTEGER ( 0 | 1..21845 )
	            UNITS      "seconds"
	            MAX-ACCESS read-write
	            STATUS     current
	            DESCRIPTION
	                    "Time interval (in seconds) for the
	                     KeepAlive timer configured for this BGP
	                     speaker with this peer.  The value of this
	                     object will only determine the
	                     KEEPALIVE messages' frequency relative to
	                     the value specified in
	                     bgpPeerHoldTimeConfigured; the actual
	                     time interval for the KEEPALIVE messages is
	                     indicated by bgpPeerKeepAlive.  A
	                     reasonable maximum value for this timer
	                     would be one third of that of
	                     bgpPeerHoldTimeConfigured.
	                     If the value of this object is zero (0),
	                     no periodical KEEPALIVE messages are sent
	                     to the peer after the BGP connection has
	                     been established.  The suggested value for
	                     this timer is 30 seconds."
	            REFERENCE
	                    "RFC 4271, Section 4.4.
	                     RFC 4271, Section 10."
	            ::= { bgpPeerEntry 24 }
            
                bgpPeerMinASOriginationInterval OBJECT-TYPE
                    SYNTAX     INTEGER (1..65535)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "Time interval in seconds for the
                            MinASOriginationInterval timer.
                            The suggested value for this timer is 15
                            seconds."
                    ::= { bgpPeerEntry 25 }    
                    
                bgpPeerMinRouteAdvertisementInterval OBJECT-TYPE
                    SYNTAX     INTEGER (1..65535)
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "Time interval in seconds for the
                            MinRouteAdvertisementInterval timer.
                            The suggested value for this timer is 30
                            seconds."
                    ::= { bgpPeerEntry 26 }

                bgpPeerInUpdateElapsedTime OBJECT-TYPE
                    SYNTAX     Gauge32
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "Elapsed time in seconds since the last BGP
                            UPDATE message was received from the peer.
                            Each time bgpPeerInUpdates is incremented,
                            the value of this object is set to zero
                            (0)."
                    ::= { bgpPeerEntry 27 }       

               bgpPeerPasswordSupport OBJECT-TYPE
                    SYNTAX     INTEGER {
                                         disable(0),
                                         enable(1)
                                       }
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "indicate if peer has been configured password"
                    ::= { bgpPeerEntry 28} 
                    
                bgpPeerPassword OBJECT-TYPE
                    SYNTAX    OCTET STRING (SIZE (1..80))
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "the MD5 password configured to peer"
                    ::= { bgpPeerEntry 29} 
                    
                 bgpPeerRtReflectorState OBJECT-TYPE
                    SYNTAX     INTEGER {
                    					disable(0),
                    					enable(1)                                         
                                       }
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "indicate peer configured as reflector client"
                    ::= { bgpPeerEntry 30 } 
                    
                  bgpPeerAttachedVrf OBJECT-TYPE
                    SYNTAX     OCTET STRING (SIZE (0..32))
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "when peer configured to a special vrf,indicate
                            vrf name"
                    ::= { bgpPeerEntry 31 } 
                    
                  bgpPeerSendCommunity OBJECT-TYPE
                    SYNTAX     INTEGER {
                                         unsend(0),
                                         send(1)
                                       }
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "if peer can send sommunity"
                    ::= { bgpPeerEntry 32}  
                    
                                   
                   bgpPeerRawStatus OBJECT-TYPE
                    SYNTAX     INTEGER {
                                        active(1),
                                        notinservice(2),
                                        notready(3),
                                        createandgo(4),
                                        createandwait(5),
                                        destory(6)
                                       }
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "if neighbor is a ipv6 neighbor"
                    ::= { bgpPeerEntry 33 }
                    
                    bgpPeerUpdateSource OBJECT-TYPE
                    SYNTAX     InetAddress
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "used for 6pe"
                    ::= { bgpPeerEntry 34 }


--new peer attribute S ddp add 20051010
                 
                 bgpPeer4BytesAsNumber OBJECT-TYPE
                    SYNTAX     INTEGER
                    MAX-ACCESS read-write
                    STATUS     current
                    DESCRIPTION
                            "peer's 4bytes AS NUmber"
                    ::= { bgpPeerEntry 35 }  

                 bgpPeer4BytesAsSupport OBJECT-TYPE
                    SYNTAX     INTEGER {support(1),not-support(0)}
                    MAX-ACCESS read-only
                    STATUS     current
                    DESCRIPTION
                            "peer's 4bytes AS NUmber capability"
                    ::= { bgpPeerEntry 36}  
                         
                 bgpPeerLocalAfiFlag OBJECT-TYPE
                    SYNTAX     INTEGER 
                    MAX-ACCESS read-write
